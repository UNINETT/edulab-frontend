{{> header}}


<div class="container">


    <div class="row">
        <div class="col-lg-12">
            <!-- Main component for a primary marketing message or call to action -->
            <div class="uninett-color-white uninett-padded gutter">

                <h1>Opprett ny lab</h1>

                <p>Som faglærer kan du enkelt opprette en maskinlab for dine studenter. Du kan her sette opp en mal og ressurspolicy for hvor mye ressurser hver student kan disponere.</p>

                <form>


                <div class="form-group">
                    <label for="labname">Navn på lab</label>
                    <input type="name" class="form-control" id="labname" placeholder="Navn på lab" value="Lab 3 i faget Maskinlæring (IEX7765)">
                </div>




                <div class="control-group">
                  <label for="select-gr">Velg undervisningsgruppe</label>
                  <select id="select-gr" placeholder="Velg gruppe..."></select>
                </div>
                <script>
                // <select id="select-tools"></select>
                $('#select-gr').selectize({
                  maxItems: 1,
                  valueField: 'id',
                  labelField: 'title',
                  searchField: 'title',
                  options: [
                      {id: 1, title: 'Maskinlæring (IEX7765)'},
                      {id: 2, title: 'Matematikk 113 (MAT113)'},
                      {id: 3, title: 'Fysikk 333 (FYI333)'}
                  ],
                  create: false
                });
                </script>





                <div class="control-group">
                    <label for="select-net">Velg nettverk og sikkerhetspolicy</label>
                    <select id="select-net" placeholder="Velg oppsett for nettverk og sikkerhet"></select>
                </div>
                <script>
                // <select id="select-tools"></select>
                $('#select-net').selectize({
                    maxItems: 1,
                    valueField: 'id',
                    labelField: 'title',
                    searchField: 'title',
                    options: [
                        {id: 1, title: 'Egne maskiner er på samme nettverk'},
                        {id: 2, title: 'Hele lab-en er på samme nettverk'},
                        {id: 3, title: 'Ingen maskiner på samme nettverk'},
                        {id: 4, title: 'Offentlig webtjener, eget nettverk'}
                    ],
                    create: false
                });
                </script>








                    <div class="control-group">
    					<label for="select-images2">Tilgjengelige maskinimage</label>
    					<select id="select-images2" class="repositories" placeholder="Gjør et utvalg av image"></select>
                        <p class="itemdesc">Du kan også velge å <a href="#">laste opp egne image</a></p>
    				</div>
    				<script>

var icons = {
    "redhat": "data:image/png;base64,",
    "suse": "data:image/png;base64,",
    "ubuntu": "data:image/png;base64,",
    "windows": "data:image/png;base64,",
    "_default": "data:image/png;base64,"
}


    				$('#select-images2').selectize({
    					maxItems: null,
    					valueField: 'id',
    					labelField: 'title',
    					searchField: 'title',
    					options: [
    						{id: 1, title: 'Microsoft Windows Server 2016', type: 'windows'},
    						{id: 2, title: 'Microsoft Windows Server 2012 R2', type: 'windows'},
    						{id: 3, title: 'Microsoft Windows Server 2008 R2', type: 'windows'},
                            {id: 4, title: 'Ubuntu Server 14.04 LTS (HVM)', type: 'ubuntu'},
                            {id: 5, title: 'Ubuntu Server 16.04 LTS (HVM)', type: 'ubuntu'},
                            {id: 6, title: 'SUSE Linux Enterprise Server 12 SP2 (HVM)', type: 'suse'},
                            {id: 7, title: 'Red Hat Enterprise Linux 7.3 (HVM)', type: 'redhat'}


    					],
                        render: {
                            option: function(item, escape) {
                                console.log("Item", item);
                                var icon = (item.type ? icons._default : icons[item.type]);
                                var icon = icons[item.type];
                                console.log("Icon", icon);
                                return '<div>' +
                                    '<span class="title">' +
                                        '<span class="name"><img src="' + icon + '" /> ' + escape(item.title) + '</span>' +
                                        '<span class="by">Amazon AWS</span>' +
                                    '</span>' +
                                    // '<span class="description">' + escape(item.desc) + '</span>' +
                                '</div>';
                            }
                        },
    					create: false
    				});
    				</script>



                    <div class="row">
                        <div class="col-md-6">
                            <div class="control-group">
                                <label for="select-it">Velg instanstype</label>
                                <select id="select-it" class="repositories" placeholder="Velg instanstype"></select>
                                <p class="itemdesc">Sjekk <a href="#">prislista</a> for detaljer.</p>
                            </div>
                            <script>

                            var types = {
                                "gen": "General Purpose",
                                "cpu": "CPU Optimized",
                                "gpu": "GPU Optimized",
                                "mem": "Memory Optimized"
                            }

                            $('#select-it').selectize({
                                maxItems: 1,
                                valueField: 'id',
                                labelField: 'title',
                                searchField: 'title',
                                options: [
                                    {id: 1, title: 't2.nano', type: 'gen', 'cpu': 1, 'mem': 0.5},
                                    {id: 2, title: 't2.micro', type: 'gen', 'cpu': 1, 'mem': 1},
                                    {id: 3, title: 't2.small', type: 'gen', 'cpu': 1, 'mem': 2},
                                    {id: 4, title: 't2.medium', type: 'gen', 'cpu': 2, 'mem': 4},
                                    {id: 5, title: 't2.large', type: 'gen', 'cpu': 2, 'mem': 8},
                                    {id: 6, title: 't2.xlarge', type: 'gen', 'cpu': 4, 'mem': 16},
                                    {id: 7, title: 'c4.large', type: 'cpu', 'cpu': 2, 'mem': 3.75},
                                    {id: 8, title: 'c4.xlarge', type: 'cpu', 'cpu': 4, 'mem': 7.5},
                                    {id: 9, title: 'g2.2xlarge', type: 'gpu', 'cpu': 8, 'mem': 15},
                                    {id:10, title: 'r4.large', type: 'mem', 'cpu': 2, 'mem': 15.25},
                                ],
                                render: {
                                    option: function(item, escape) {
                                        console.log("Item", item)
                                        var icon = (item.type ? icons._default : icons[item.type])
                                        var icon = icons[item.type]
                                        console.log("Icon", icon)
                                        return '<div>' +
                                            '<span class="title">' +
                                                '<span class="name"><i style="color: #533" class="glyphicon glyphicon-cloud"></i> ' + escape(item.title) + '</span>' +
                                            '</span>' +
                                            // '<span class="description">' + escape(item.desc) + '</span>' +
                                            '<ul class="meta">' +
                                                '<li class="language">' + types[item.type] + '</li>'+
                                                '<li class="watchers"><span>' + escape(item.cpu) + '</span> CPUs</li>' +
                                                '<li class="forks"><span>' + escape(item.mem) + '</span> GiB memory</li>' +
                                            '</ul>' +
                                        '</div>'
                                    }
                                },
                                create: false
                            })
                            </script>


                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                              <label for="instance-num">Max instanser per elev</label>
                              <input type="number" class="form-control" id="instance-num"  placeholder="Max instanser" min="1" max="99" value="1">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                              <label for="instance-tot">Max instanser totalt</label>
                              <input type="number" class="form-control" id="instance-tot"  placeholder="Max instanser" min="1" max="99" value="99">
                            </div>
                        </div>

                    </div>


                    <div class="row">

                        <div class="col-md-6">

                            <div class="form-group">
                              <label for="period-from">Oppstartsdato</label>
                              <input type="text" class="form-control" id="period-from">
                              <p class="itemdesc">Studentene vil ikke kunne opprette noen maskiner før ved midnatt denne datoen.</p>
                            </div>

                        </div>

                        <div class="col-md-6">

                            <div class="form-group">
                              <label for="period-to">Nedstenging av lab</label>
                              <input type="text" class="form-control" id="period-to">
                              <p class="itemdesc">Etter datoen for nedstenging av lab-en vil alle maskinene automatisk slettes.</p>
                            </div>

                        </div>


                    </div>



                    <script>
                    var date = new Date();
                    var opts = {
                        format: "yyyy-mm-dd",
                        language: "no",
                        calendarWeeks: true,
                        autoclose: true,
                        todayHighlight: true,
                        startDate: date
                    };
                    $('#period-from').datepicker(opts);
                    $('#period-to').datepicker(opts);

                    </script>






                  <!-- <button type="submit" class="btn btn-default">Submit</button> -->
                  <p class="gutter">
                      <a class="btn btn-primary " target="_blank"
                          href="#" role="button">
                          <span class="glyphicon glyphicon-cloud"></span> Opprett ny lab</a>
                  </p>
                </form>

                <p></p>

            </div>
        </div>
    </div>

</div>





{{> footer}}
